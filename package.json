{
  "author": "Felix Geisend√∂rfer <felix@debuggable.com> (http://debuggable.com/)",
  "name": "@yaorg/measured-core",
  "description": "A Node library for measuring and reporting application-level metrics.",
  "version": "1.3.0",
  "homepage": "https://yaorg.github.io/node-measured/",
  "engines": {
    "node": ">= 5.12"
  },
  "main": "./index",
  "scripts": {
    "clean": "rm -fr build",
    "format": "prettier --write '**/*.{ts,js}'",
    "lint": "eslint lib/**/*.js",
    "watch": "npm run test:node -- --watch",
    "test:node": "mocha './test/**/test-*.js'",
    "test:node:coverage": "nyc --report-dir build/coverage/ --reporter=html --reporter=text mocha './test/**/test-*.js'",
    "test:browser": "mochify './test/**/test-*.js'",
    "wd": "mochify --wd './test/**/test-*.js'",
    "jsdoc": "jsdoc --readme ./README.md --recurse --destination ./build/jsdoc/ ./lib/ ./index.js",
    "deploy-js-doc-to-github": "gh-pages -d build/jsdoc",
    "test": "scripts/test.sh",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "binary-search": "^1.3.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.1",
    "gh-pages": "^1.1.0",
    "jsdoc": "^3.5.5",
    "jslint": "^0.8",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "mochify": "^5.6.1",
    "nyc": "^11.8.0",
    "optional-js": "^2.0.0",
    "prettier": "^1.12.1",
    "sinon": "^5.0.7"
  },
  "repository": {
    "url": "git://github.com/felixge/node-measured.git"
  },
  "files": [
    "index.js",
    "lib",
    "Readme.md",
    "CHANGES.md"
  ],
  "license": "MIT"
}
